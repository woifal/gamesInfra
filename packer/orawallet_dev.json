{
    "builders": [
        {
            "type": "docker",
            "image": "woifal/oraclient:1.3",
            "commit": "true",
            "pull": "false"
        }
    ],
    "provisioners": [
        {
            "type": "file",
            "source": "files/Wallet_gamesDevDB.zip",
            "destination": "/tmp/Wallet_gamesBackend.zip"
        },
        {
            "type": "ansible-local",
            "playbook_file": "ansible/orawallet.yml",
            "extra_arguments": [
                "-vvv"
            ]
        }
    ],
    "post-processors": [
        [
            {
                "type": "docker-tag",
                "repository": "woifal/orawallet_dev",
                "tag": "1.3"
           }
        ]
    ]
}